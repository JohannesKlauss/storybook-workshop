{
  "name": "storybook-workshop",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.6.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "7.12.2",
    "react-icons": "4.2.0",
    "react-scripts": "4.0.3",
    "recoil": "^0.4.0",
    "typescript": "4.3.5",
    "uuid": "8.3.2",
    "web-vitals": "2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic --project-token=b35a703f5071"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
